<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox fx:id="fond" alignment="CENTER" prefHeight="514.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FXMLSaveDataBaseController">
   <children>
      <Label fx:id="labelScore" alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="42.0" text="Score">
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </Label>
      <Label fx:id="instructions" text="Si vous souhaitez enregistrer votre score, entrez votre pseudo, sinon quittez." wrapText="true">
         <padding>
            <Insets bottom="20.0" left="50.0" right="50.0" />
         </padding>
      </Label>
      <TextField fx:id="pseudo" prefHeight="31.0" text="Votre pseudo ">
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="20.0" />
         </VBox.margin>
      </TextField>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="btnValider" mnemonicParsing="false" text="Valider" />
            <Button fx:id="btnQuitter" mnemonicParsing="false" text="Quitter" />
         </children>
         <padding>
            <Insets top="50.0" />
         </padding>
      </HBox>
   </children>
</VBox>
